# NexoraSIM Nexora Agent Configuration Template
# Copy this file to .env and configure with your API keys

# Agent Configuration
DEFAULT_PROVIDER=openai
FALLBACK_PROVIDERS=google_gemini,groq,anthropic
MAX_RETRIES=3
REQUEST_TIMEOUT=30
LOG_LEVEL=INFO

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4
OPENAI_ENABLED=true
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=2000
OPENAI_TIMEOUT=30
OPENAI_RETRIES=3

# Google Gemini Configuration
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_MODEL=gemini-pro
GEMINI_ENABLED=true
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_TOKENS=2000
GEMINI_TIMEOUT=30
GEMINI_RETRIES=3

# xAI Grok Configuration
XAI_API_KEY=your_xai_api_key_here
XAI_ENDPOINT=https://api.x.ai/v1
XAI_MODEL=grok-1
XAI_ENABLED=false
XAI_TEMPERATURE=0.7
XAI_MAX_TOKENS=2000
XAI_TIMEOUT=30
XAI_RETRIES=3

# Groq Configuration
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=mixtral-8x7b-32768
GROQ_ENABLED=false
GROQ_TEMPERATURE=0.7
GROQ_MAX_TOKENS=2000
GROQ_TIMEOUT=30
GROQ_RETRIES=3

# Anthropic Claude Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-sonnet-20240229
ANTHROPIC_ENABLED=false
ANTHROPIC_TEMPERATURE=0.7
ANTHROPIC_MAX_TOKENS=2000
ANTHROPIC_TIMEOUT=30
ANTHROPIC_RETRIES=3

# Generic HTTP Provider Configuration
GENERIC_API_KEY=your_generic_api_key_here
GENERIC_ENDPOINT=https://your-custom-api.com/v1/completions
GENERIC_ENABLED=false
GENERIC_MODEL=default
GENERIC_TEMPERATURE=0.7
GENERIC_MAX_TOKENS=2000
GENERIC_TIMEOUT=30
GENERIC_RETRIES=3
GENERIC_REQUEST_FORMAT=openai
GENERIC_RESPONSE_FORMAT=auto
GENERIC_TEXT_PATH=text
GENERIC_MODEL_PATH=model
GENERIC_TOKENS_PATH=tokens_used
GENERIC_HEADERS={}

# Audit and Security Configuration
AUDIT_ENABLED=true
AUDIT_LOG_FILE=audit.log
PERFORMANCE_MONITORING=true
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=60
API_KEY_ROTATION_DAYS=90

# Do not commit real secrets. Copy this file to .env and edit locally.
