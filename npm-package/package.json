{
  "name": "@nexorasim/nexora-agent",
  "version": "1.0.0",
  "description": "Enterprise-grade AI deployment and orchestration platform with multi-provider support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "lint": "eslint src/**/*.ts --max-warnings=0 || exit 0",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "prepublishOnly": "npm run clean && npm run build",
    "version:patch": "npm version patch && git push origin --tags",
    "version:minor": "npm version minor && git push origin --tags",
    "version:major": "npm version major && git push origin --tags",
    "audit:security": "npm audit --audit-level moderate || true",
    "audit:licenses": "license-checker --summary || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nexorasim/nexorasim.github.io.git",
    "directory": "npm-package"
  },
  "keywords": [
    "ai",
    "agent",
    "orchestration",
    "multi-provider",
    "openai",
    "gemini",
    "grok",
    "enterprise",
    "automation",
    "mcp"
  ],
  "author": "NexoraSIM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nexorasim/nexorasim.github.io/issues"
  },
  "homepage": "https://nexorasim.github.io",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "license-checker": "^25.0.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}